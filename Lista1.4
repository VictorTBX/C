#include <stdio.h>
#include <stdlib.h>

int main()
{
    int popA = 90000000, popB = 20000000, ano = 0;

    while (popA > popB) {
        popA = popA * 1,01;
        popB = popB * 1,03;
        ano++;
    }

    printf("Levarao %d anos para a populacao B ultrapassar a populacao A", ano);

    return 0;
}
