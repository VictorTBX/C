#include <stdio.h>
#include <stdlib.h>

int main()
{
	int n = 2, x, y, i, j, g, teste = 0, saldo = 0, maiorSaldo;

	while (n != 0){
		n = 1001;
		i = 0;
		j = 0;
		while (n > 1000 || n < 0)
			scanf("%d", &n);

		printf("%d \n", n);

		if(n == 0)
            break;

		for (g = 0; g < n; g++)
		{
		    x = 51;
            y = 51;
		    while (x > 50 || x < 0)
                scanf("%d", &x);
			while (y > 50 || y < 0)
                scanf("%d", &y);
			printf("%d %d\n", x, y);

			if(i == 0) {
                if(x-y > 0){
                    i = g+1;
                    j = g+1;
                    saldo = x-y;
                    maiorSaldo = saldo;
                }
            } else {
                saldo = saldo +x -y;
                if(saldo >= maiorSaldo){
                    maiorSaldo = saldo;
                    j = g+1;
                }
            }
        }

		teste++;
		printf("Teste %d\n", teste);
		if (i > 0) {
            printf("%d %d\n", i, j);
		}
		else
			printf("Nenhum\n");
	}

	return 0;
}
