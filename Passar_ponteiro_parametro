#include <stdio.h>
#include <stdlib.h>

void quadrado(int *a){
    *a = (*a)*(*a);
}

int main()
{
    int numero, numero2;
    char c = 253;

    printf("Informe o valor:");
    scanf("%d", &numero);
    numero2 = numero;
    quadrado(&numero);
    printf("%d%c = %d ", numero2, c, numero);

    return 0;
}
